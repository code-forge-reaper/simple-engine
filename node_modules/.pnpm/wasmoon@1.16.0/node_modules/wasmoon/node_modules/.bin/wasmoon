#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/cross/projects/spring-yard/node_modules/.pnpm/wasmoon@1.16.0/node_modules/wasmoon/bin/node_modules:/home/cross/projects/spring-yard/node_modules/.pnpm/wasmoon@1.16.0/node_modules/wasmoon/node_modules:/home/cross/projects/spring-yard/node_modules/.pnpm/wasmoon@1.16.0/node_modules:/home/cross/projects/spring-yard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/cross/projects/spring-yard/node_modules/.pnpm/wasmoon@1.16.0/node_modules/wasmoon/bin/node_modules:/home/cross/projects/spring-yard/node_modules/.pnpm/wasmoon@1.16.0/node_modules/wasmoon/node_modules:/home/cross/projects/spring-yard/node_modules/.pnpm/wasmoon@1.16.0/node_modules:/home/cross/projects/spring-yard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/wasmoon" "$@"
else
  exec node  "$basedir/../../bin/wasmoon" "$@"
fi
